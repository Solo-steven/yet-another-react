{
    "root": true,
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
      "env": {
      "browser": true,
      "node": true
    },
    "rules": {
          "semi": "warn"
      }
  }